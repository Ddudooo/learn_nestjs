{
  "name": "express-practice",
  "version": "0.0.1",
  "description": "익스프레스 연습.",
  "main": "index.js",
  "packageManager": "yarn@3.1.0",
  "scripts": {
    "lint": "eslint \"{src, test}/**/*.ts\" --fix",
    "prebuild": "rimraf dist",
    "build": "yarn pnpify tsc",
    "prestart": "yarn build",
    "start:dev": "yarn pnpify tsc-watch --onSuccess \"node dist/src/app.js\"",
    "start": "NODE_OPTIONS=\"--require $(pwd)/.pnp.cjs\" node dist/src/app.js",
    "test": "test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@yarnpkg/pnpify": "^3.1.1-rc.5",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "tsc": "^2.0.3",
    "tsc-watch": "^4.5.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2"
  },
  "_moduleAliases": {
    "@" : "dist/src"
  }
}
